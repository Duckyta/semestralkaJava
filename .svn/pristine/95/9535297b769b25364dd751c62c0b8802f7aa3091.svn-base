package komparatory;

import java.util.Comparator;
import pobocka.IPobocka;

/**
 *
 * @author DuckMcQuack
 */
public class KomparatorCislo  implements Comparator<IPobocka>{

    @Override
    public int compare(IPobocka t, IPobocka t1) {
        if (t.getPocetZamestnancu() > t1.getPocetZamestnancu()) {
            return 1;
        }
        else if (t.getPocetZamestnancu() == t1.getPocetZamestnancu())
            return 0;
        else
            return -1;
    }
    
}
